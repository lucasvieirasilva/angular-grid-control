{
  "name": "angular-grid-control",
  "dependencies": {
    "angular": "1.4.x",
    "angular-translate": "^2.9.0",
    "angular-animate": "1.4.x",
    "angular-bootstrap": "1.3.3",
    "bootstrap": "3.3.5",
    "jquery": "3.1.1"
  },
  "main": [
    "dist/angular-grid-control.min.js",
    "src/angular-grid-control.js",
    "dist/css/angular-grid-control.min.css",
    "src/css/angular-grid-control.css"
  ],
  "authors": [{
    "name": "Lucas Vieira da Silva",
    "email": "lucas.vieira94@outlook.com",
    "homepage": "https://lucasvieirasilva.github.io"
  }, {
    "name": "Anderson Saraiva Lima",
    "email": "a.saraiva90@gmail.com",
    "homepage": "https://andersonsaraiva.github.io"
  }],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "sample",
    "src",
    "/*",
    "!dist",
    "!bower.json"
  ],
  "resolutions": {
    "angular": "~1.4.x"
  },
  "exportsOverride": {
