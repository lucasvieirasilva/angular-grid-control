      "css": "angular-csp.css"
    },
    "angular-animate": {
      "": "angular-animate{.js,.min.js,.min.js.map, .min.js.gzip}"
    },
    "angular-bootstrap": {
      "": "ui-bootstrap-tpls{.js,.min.js,.min.js.map, .min.js.gzip}",
      "css": "ui-bootstrap-csp{.css,.min.css,.min.css.map, .min.css.gzip}"
    },
    "angular-translate": {
      "": "angular-translate{.js,.min.js,.min.js.map, .min.js.gzip}"
    }
  }
}